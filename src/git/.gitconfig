[core]
  excludesfile = ~/.gitignore_global
  editor = nvim
[user]
  name = Jason Di Benedetto
  email = jason.dibenedetto6@gmail.com
  username = jasondibenedetto
[color]
  ui = true
[push]
  default = current
[alias]
  mr = !sh -c 'git fetch $1 merge-requests/$2/head:mr-$1-$2 && git checkout mr-$1-$2' -
  pr = !sh -c 'git fetch $1 pull/$2/head:pr-$1-$2 && git checkout pr-$1-$2' -
[rebase]
  autoStash = true
